{% trans_default_domain('service.admin') %}

{# ---- ШАБЛОН ------------------------------------------------------------ #}
{% extends Template('/admin/base.html.twig') %}

{% block title %}
    {# Добавить услугу #}
    {{ 'admin.new'|trans }}
{% endblock %}


{# ---- CSS ------------------------------------------------------------ #}
{% block stylesheets %}
    <link href="/assets/plugins/datepicker/datepicker.min.css" rel="stylesheet">
    <link href="/assets/plugins/nice-select2/nice-select2.min.css" rel="stylesheet">
{% endblock %}


{# ---- КОНТЕНТ-БЛОК ------------------------------------------------------------ #}
{% block content %}
    {{ include(_self|replace({ "template.html.twig": baks_settings.device~"/content.html.twig" })) }}
{% endblock %}

{% block javascript %}

<script nonce='{{ csp_nonce() }}' async  data-src="/assets/services/services.min.js?v={{ version }}"  class="lazy"></script>
{% endblock %}
